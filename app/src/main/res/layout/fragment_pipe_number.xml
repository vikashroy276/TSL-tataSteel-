<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white"
    tools:context=".activities.searchProcTallyPipe.searchPipeNumber.PipeNumberFragment">


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="none"
        android:fillViewport="true">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_marginEnd="10dp"
            android:layout_weight=".12"
            android:paddingBottom="10dp"
            android:paddingEnd="5dp"
            android:paddingStart="5dp"
            android:layout_marginStart="10dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:orientation="vertical"
                android:layout_weight=".1"
                android:layout_marginTop="15dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="20dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/pipeNo"
                        android:textStyle="bold"
                        android:gravity="center|start"
                        android:textColor="@color/darkGreyColor"
                        android:textSize="14sp"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=" [ put comma(,) for multiple pipe ]"
                        android:textStyle="bold"
                        android:visibility="gone"
                        android:gravity="center|start"
                        android:textColor="@color/darkGreyColor"
                        android:textSize="14sp"/>
                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text="@string/auto_complete"
                        android:layout_weight=".17"
                        android:visibility="invisible"
                        android:gravity="center|end"
                        android:textColor="@color/redColor"
                        android:textSize="14sp"/>
                </LinearLayout>
                <com.google.android.material.card.MaterialCardView
                    android:id="@+id/edtUserName"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="7dp"
                    android:layout_marginTop="10dp"
                    app:strokeColor="@color/greyMediumColor"
                    app:strokeWidth="1dp">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:gravity="center">
                        <EditText
                            android:id="@+id/pipeNumTxt"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center|start"
                            android:layout_weight=".1"
                            android:drawableStart="@drawable/leftdrawableuser"
                            android:hint="@string/pipeNo"
                            android:paddingStart="8dp"
                            android:imeOptions="actionDone"
                            android:singleLine="true"
                            android:drawablePadding="9dip"
                            android:backgroundTint="@color/transparent"
                            android:paddingEnd="2dp"
                            android:textColor="@color/black"
                            android:textSize="11sp"
                            android:textStyle="bold" />
                    </LinearLayout>
                </com.google.android.material.card.MaterialCardView>
                <com.google.android.material.card.MaterialCardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardBackgroundColor="@color/transparent"
                    app:cardCornerRadius="5dp"
                    android:layout_marginTop="15dp"
                    app:strokeColor="@color/greyMediumColor"
                    app:strokeWidth="1dp">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:background="@color/greyMediumColor"
                            android:orientation="horizontal">
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="center|start"
                                android:layout_weight=".22"
                                android:text="@string/s_no"
                                android:paddingStart="10dp"
                                android:textColor="@color/black"
                                android:textSize="12sp"
                                android:textStyle="bold" />
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="center|start"
                                android:layout_weight=".2"
                                android:text="@string/pipeNo"
                                android:textColor="@color/black"
                                android:textSize="12sp"
                                android:textStyle="bold" />
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:text="@string/process_sheet"
                                android:paddingEnd="10dp"
                                android:layout_weight=".1"
                                android:textColor="@color/black"
                                android:textSize="12sp"
                                android:textStyle="bold" />
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:orientation="horizontal">
                            <TextView
                                android:id="@+id/sNoTxt"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="center|start"
                                android:layout_weight=".22"
                                android:text=""
                                android:paddingStart="10dp"
                                android:textColor="@color/black"
                                android:textSize="12sp"
                                android:textStyle="bold" />
                            <TextView
                                android:id="@+id/pipeNoTxt"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="center|start"
                                android:layout_weight=".2"
                                android:text=""
                                android:textColor="@color/black"
                                android:textSize="12sp"
                                android:textStyle="bold" />
                            <TextView
                                android:id="@+id/procSheetCode"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:text=""
                                android:paddingEnd="10dp"
                                android:layout_weight=".1"
                                android:textColor="@color/black"
                                android:textSize="12sp"
                                android:textStyle="bold" />
                        </LinearLayout>
                    </LinearLayout>

                </com.google.android.material.card.MaterialCardView>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center|start"
                    android:text="@string/clientName"
                    android:textColor="@color/darkGreyColor"
                    android:textSize="14sp"
                    android:layout_marginTop="10dp"
                    android:textStyle="bold" />
                <TextView
                    android:id="@+id/clientNameTxt"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center|start"
                    android:text=""
                    android:textColor="@color/black"
                    android:textSize="12sp"
                    android:layout_marginTop="4dp"
                    android:textStyle="bold" />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center|start"
                    android:text="@string/projectName"
                    android:textColor="@color/darkGreyColor"
                    android:textSize="14sp"
                    android:layout_marginTop="10dp"
                    android:textStyle="bold" />
                <TextView
                    android:id="@+id/projectNameTxt"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center|start"
                    android:text=""
                    android:textColor="@color/black"
                    android:textSize="12sp"
                    android:layout_marginTop="4dp"
                    android:textStyle="bold" />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center|start"
                    android:text="@string/pipeSize"
                    android:textColor="@color/darkGreyColor"
                    android:textSize="14sp"
                    android:layout_marginTop="10dp"
                    android:textStyle="bold" />
                <TextView
                    android:id="@+id/pipeSizeTxt"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center|start"
                    android:text=""
                    android:textColor="@color/black"
                    android:textSize="12sp"
                    android:layout_marginTop="4dp"
                    android:textStyle="bold" />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="30dp">
                    <com.google.android.material.card.MaterialCardView
                        android:layout_width="match_parent"
                        android:layout_height="100dp"
                        android:layout_marginTop="10dp"
                        app:cardCornerRadius="8dp"
                        android:layout_weight=".1"
                        android:layout_marginEnd="10dp"
                        app:cardElevation="5dp"
                        app:strokeColor="@color/greyColor"
                        app:cardBackgroundColor="@color/blueColor">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal"
                            android:layout_margin="5dp"
                            android:paddingTop="10dp">
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="vertical"
                                android:layout_weight=".1">
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="@string/tagged_by"
                                    android:textStyle="bold"
                                    android:paddingStart="10dp"
                                    android:gravity="center|start"
                                    android:textColor="@color/white"
                                    android:textSize="13sp"/>
                                <TextView
                                    android:id="@+id/taggedByName"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text=""
                                    android:textStyle="bold"
                                    android:gravity="center"
                                    android:layout_marginTop="20dp"
                                    android:textColor="@color/white"
                                    android:textSize="11sp"/>
                            </LinearLayout>
                            <com.google.android.material.imageview.ShapeableImageView
                                android:layout_width="33dp"
                                android:layout_height="33dp"
                                android:layout_gravity="center"
                                android:layout_marginEnd="8dp"
                                android:src="@drawable/taggedbyicon"/>

                        </LinearLayout>
                    </com.google.android.material.card.MaterialCardView>
                    <com.google.android.material.card.MaterialCardView
                        android:layout_width="match_parent"
                        android:layout_height="100dp"
                        android:layout_marginTop="10dp"
                        app:cardCornerRadius="8dp"
                        android:layout_weight=".1"
                        android:layout_marginEnd="10dp"
                        app:cardElevation="5dp"
                        app:strokeColor="@color/greyColor"
                        app:cardBackgroundColor="@color/redColor">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal"
                            android:layout_margin="5dp"
                            android:paddingTop="10dp">
                            <com.google.android.material.imageview.ShapeableImageView
                                android:layout_width="33dp"
                                android:layout_height="33dp"
                                android:layout_gravity="center"
                                android:paddingStart="9dp"
                                android:src="@drawable/calendericon"/>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="vertical"
                                android:layout_weight=".1">
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="@string/tagged_on"
                                    android:textStyle="bold"
                                    android:paddingEnd="10dp"
                                    android:gravity="center|end"
                                    android:textColor="@color/white"
                                    android:textSize="13sp"/>
                                <TextView
                                    android:id="@+id/taggedondate"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text=""
                                    android:textStyle="bold"
                                    android:gravity="center"
                                    android:layout_marginTop="20dp"
                                    android:textColor="@color/white"
                                    android:textSize="11sp"/>
                            </LinearLayout>

                        </LinearLayout>
                    </com.google.android.material.card.MaterialCardView>
                </LinearLayout>
                <com.google.android.material.card.MaterialCardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardBackgroundColor="@color/navColor"
                    app:cardCornerRadius="7dp"
                    android:layout_marginTop="15dp"
                    app:strokeColor="@color/navShadowColor"
                    app:strokeWidth="1dp">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:layout_margin="16dp">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center|start"
                            android:text="@string/passed_through"
                            android:textColor="@color/white"
                            android:textSize="14sp"
                            android:textStyle="bold" />
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1.5dp"
                            android:layout_marginTop="6dp"
                            android:background="@color/navShadowColor"/>
                        <com.google.android.material.card.MaterialCardView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:cardBackgroundColor="@color/navColor"
                            app:cardCornerRadius="7dp"
                            android:layout_marginTop="15dp"
                            app:strokeColor="@color/navShadowColor"
                            app:strokeWidth="1dp">
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="vertical">
                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="45dp"
                                    android:orientation="horizontal"
                                    android:gravity="center"
                                    android:background="@color/navShadowColor">
                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:gravity="center|left"
                                        android:layout_weight=".2"
                                        android:text="@string/pStation"
                                        android:paddingStart="15dp"
                                        android:textColor="@color/white"
                                        android:textSize="12sp"
                                        android:textStyle="bold" />
                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:gravity="center|end"
                                        android:layout_weight=".11"
                                        android:paddingEnd="15dp"
                                        android:text="@string/dTime"
                                        android:textColor="@color/white"
                                        android:textSize="12sp"
                                        android:textStyle="bold" />
                                </LinearLayout>
                                <androidx.recyclerview.widget.RecyclerView
                                    android:id="@+id/stationrecycler"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    tools:listitem="@layout/remarkslayoutrecycler"/>
                            </LinearLayout>
                        </com.google.android.material.card.MaterialCardView>
                    </LinearLayout>
                </com.google.android.material.card.MaterialCardView>
                <com.google.android.material.card.MaterialCardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardBackgroundColor="@color/yellowColor"
                    app:cardCornerRadius="7dp"
                    android:layout_marginTop="15dp"
                    app:strokeColor="@color/navColor"
                    app:strokeWidth="1dp">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:layout_margin="16dp">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center|start"
                            android:text="@string/currently_at"
                            android:textColor="@color/navColor"
                            android:textSize="14sp"
                            android:textStyle="bold" />
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1.5dp"
                            android:layout_marginTop="6dp"
                            android:background="@color/navColor"/>
                        <com.google.android.material.card.MaterialCardView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:cardBackgroundColor="@color/yellowColor"
                            app:cardCornerRadius="7dp"
                            android:layout_marginTop="15dp"
                            app:strokeColor="@color/navColor"
                            app:strokeWidth="1dp">
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="vertical">
                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="45dp"
                                    android:orientation="horizontal"
                                    android:gravity="center"
                                    android:background="@color/yellowShadowColor">
                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:gravity="center|left"
                                        android:layout_weight=".2"
                                        android:text="@string/station"
                                        android:paddingStart="15dp"
                                        android:textColor="@color/white"
                                        android:textSize="12sp"
                                        android:textStyle="bold" />
                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:gravity="center|end"
                                        android:layout_weight=".11"
                                        android:paddingEnd="8dp"
                                        android:text="@string/dTime"
                                        android:textColor="@color/white"
                                        android:textSize="12sp"
                                        android:textStyle="bold" />
                                </LinearLayout>
                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="45dp"
                                    android:orientation="horizontal"
                                    android:gravity="center">
                                    <TextView
                                        android:id="@+id/currentStationName"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:gravity="center|left"
                                        android:layout_weight=".2"
                                        android:text=""
                                        android:paddingStart="15dp"
                                        android:textColor="@color/navColor"
                                        android:textSize="12sp" />
                                    <TextView
                                        android:id="@+id/currentTestDate"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:gravity="center|end"
                                        android:layout_weight=".11"
                                        android:paddingEnd="10dp"
                                        android:text=""
                                        android:textColor="@color/navColor"
                                        android:textSize="12sp" />
                                </LinearLayout>
                            </LinearLayout>
                        </com.google.android.material.card.MaterialCardView>
                    </LinearLayout>
                </com.google.android.material.card.MaterialCardView>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="15dp">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight=".1"
                            android:text="@string/remarks"
                            android:layout_marginTop="6dp"
                            android:textColor="@color/black"
                            android:textSize="14sp"
                            android:textStyle="bold" />
                        <com.google.android.material.card.MaterialCardView
                            android:id="@+id/Add_btn"
                            android:layout_width="90dp"
                            android:layout_height="35dp"
                            app:cardBackgroundColor="@color/purpleColor"
                            app:cardCornerRadius="7dp"
                            android:layout_marginEnd="5dp"
                            app:strokeColor="@color/purpleColor"
                            app:strokeWidth="1dp">

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:drawablePadding="9dip"
                                android:gravity="center"
                                android:hint="@string/add"
                                android:paddingStart="10dp"
                                android:paddingEnd="8dp"
                                android:textColor="@color/white"
                                android:textColorHint="@color/white"
                                android:textSize="14sp"
                                android:textStyle="bold" />
                        </com.google.android.material.card.MaterialCardView>
                        <com.google.android.material.card.MaterialCardView
                            android:id="@+id/Show_btn"
                            android:layout_width="90dp"
                            android:layout_height="35dp"
                            app:cardBackgroundColor="@color/purpleColor"
                            app:cardCornerRadius="7dp"
                            android:layout_marginStart="5dp"
                            app:strokeColor="@color/purpleColor"
                            app:strokeWidth="1dp">

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:drawablePadding="9dip"
                                android:gravity="center"
                                android:hint="@string/show"
                                android:paddingStart="10dp"
                                android:paddingEnd="8dp"
                                android:textColor="@color/white"
                                android:textColorHint="@color/white"
                                android:textSize="14sp"
                                android:textStyle="bold" />
                        </com.google.android.material.card.MaterialCardView>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/remarkLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:visibility="gone">
                        <com.google.android.material.card.MaterialCardView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:cardBackgroundColor="@color/transparent"
                            app:cardCornerRadius="5dp"
                            android:layout_marginTop="15dp"
                            app:strokeColor="@color/greyMediumColor"
                            app:strokeWidth="1dp">
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="vertical">
                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="40dp"
                                    android:background="@color/greyMediumColor"
                                    android:orientation="horizontal">
                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:gravity="center|start"
                                        android:layout_weight=".22"
                                        android:text="@string/s_no"
                                        android:paddingStart="10dp"
                                        android:textColor="@color/black"
                                        android:textSize="12sp"
                                        android:textStyle="bold" />
                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:gravity="center|start"
                                        android:layout_weight=".18"
                                        android:text="@string/pipe_no"
                                        android:textColor="@color/black"
                                        android:textSize="12sp"
                                        android:textStyle="bold" />
                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:gravity="center"
                                        android:text="@string/process_sheet"
                                        android:paddingEnd="10dp"
                                        android:layout_weight=".11"
                                        android:textColor="@color/black"
                                        android:textSize="12sp"
                                        android:textStyle="bold" />
                                </LinearLayout>
                                <androidx.recyclerview.widget.RecyclerView
                                    android:id="@+id/remarksRecycler"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    tools:listitem="@layout/remarkslayoutrecycler"/>
                            </LinearLayout>

                        </com.google.android.material.card.MaterialCardView>
                    </LinearLayout>
                    <com.google.android.material.card.MaterialCardView
                        android:id="@+id/remarkCard"
                        android:layout_width="match_parent"
                        android:layout_height="90dp"
                        app:cardBackgroundColor="@color/transparent"
                        app:cardCornerRadius="10dp"
                        android:elevation="3dp"
                        android:visibility="gone"
                        android:layout_marginTop="10dp"
                        app:strokeColor="@color/greyColor"
                        app:strokeWidth="1dp">

                        <EditText
                            android:id="@+id/remarks_edt"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:drawablePadding="9dip"
                            android:gravity="center|left"
                            android:hint="@string/remarks"
                            android:paddingStart="10dp"
                            android:paddingEnd="8dp"
                            android:backgroundTint="@color/transparent"
                            android:textColor="@color/black"
                            android:textColorHint="@color/black"
                            android:textSize="14sp"
                            android:textStyle="bold" />
                    </com.google.android.material.card.MaterialCardView>
                </LinearLayout>
            </LinearLayout>
            <com.google.android.material.card.MaterialCardView
                android:id="@+id/Submit_btn"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                app:cardBackgroundColor="@color/blueColor"
                app:cardCornerRadius="7dp"
                android:layout_marginTop="20dp"
                app:strokeColor="@color/blueColor"
                app:strokeWidth="1dp">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:drawablePadding="9dip"
                    android:gravity="center"
                    android:hint="@string/submit"
                    android:paddingStart="10dp"
                    android:paddingEnd="8dp"
                    android:textColor="@color/white"
                    android:textColorHint="@color/white"
